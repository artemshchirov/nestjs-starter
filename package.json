{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "NestJS boilerplate. Auth, Prisma, Postgres, Mailing, I18N, Docker",
  "author": "Artem Shchirov",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:db": "npm run docker:up && npm run db:generate && npm run db:seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "prisma:studio": "npx prisma studio",
    "db:seed": "prisma db seed",
    "db:seed:create": "hygen seeds create",
    "db:reset": "prisma migrate reset --preview-feature",
    "db:migrate:dev": "prisma migrate dev --preview-feature",
    "db:migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "db:migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "db:migrate:status": "npx prisma migrate status --preview-feature",
    "db:migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "db:generate": "npx prisma generate",
    "db:generate:watch": "npx prisma generate --watch",
    "test": "jest --config test/jest-e2e.json ",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "env:local:up": "docker-compose up -d",
    "env:local:down": "docker-compose down",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker:build": "docker-compose build",
    "postinstall": "npm run db:generate",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.587.0",
    "@nestjs/common": "10.3.8",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.8",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/typeorm": "10.0.2",
    "@prisma/client": "^5.14.0",
    "@types/multer-s3": "3.0.3",
    "apple-signin-auth": "1.7.6",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fb": "2.0.0",
    "google-auth-library": "9.10.0",
    "handlebars": "4.7.8",
    "ms": "2.1.3",
    "multer": "1.4.5-lts.1",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.4.5",
    "nestjs-prisma": "^0.23.0",
    "nodemailer": "6.9.13",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "pg": "8.11.5",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.7",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.0",
    "twitter": "1.7.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.8",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/facebook-js-sdk": "3.3.11",
    "@types/jest": "29.5.12",
    "@types/ms": "0.7.34",
    "@types/multer": "1.4.11",
    "@types/node": "20.12.13",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "@types/twitter": "1.7.4",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "env-cmd": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prisma": "^5.14.0",
    "supertest": "7.0.0",
    "ts-jest": "29.1.4",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.4.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
